freq = 600;
phase = 0;
omega = 2 * pi * freq;
passo = 0.00001;
tfinal = 2;
time = 0 : passo: tfinal/freq;
seno = 0.5 + (2/pi)*sin(omega*time+phase) + (2/(3*pi))*sin(3*omega*time + phase) + (2/(5*pi))*sin(5*omega*time + phase) + (2/(7*pi))*sin(7*omega*time + phase);
clf
plot(time,seno,'k','LineWidth',2);
