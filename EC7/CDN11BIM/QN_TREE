Fs = 600 ;                   
   dt = 1/Fs;                   
   StopTime = 1.00;             
   t = (0:dt:StopTime-dt)';     
   
   Fc = 600;                     
   x = 0.5 + (2/pi)*sin(2*pi*Fc*t) + (2/(3*pi))*sin(3*2*pi*Fc*t) + (2/(5*pi))*sin(5*2*pi*Fc*t) + (2/(7*pi))*sin(7*2*pi*Fc*t);
      
   figure;
   plot(t,x,'black');
      
   xlabel('t(s)');
   ylabel('f(t)');
   zoom xon;
